<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cy.db.sys.dao.SysRoleMenuDao">
    <delete id="deleteRoleMenuObjectByMenuId">
        DELETE
        FROM sys_role_menus
        WHERE menu_id=#{id}
    </delete>
</mapper>